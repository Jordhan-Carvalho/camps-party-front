ront/camps-party-front/src/contexts/UserContext.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/components/AuthenticatedRoute.js",["103"],"import React, { useContext, useEffect } from \"react\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport jwt from \"jwt-decode\";\n\nimport { userContext } from \"../contexts/UserContext\";\n\nexport default function AuthenticatedRoute({ children, ...rest }) {\n  const { user, setUser } = useContext(userContext);\n\n  useEffect(() => {\n    verifyTokenExp();\n  }, []);\n\n  const verifyTokenExp = () => {\n    if (jwt(user.token).exp < Date.now() / 1000) {\n      setUser(null);\n    }\n  };\n\n  return (\n    <Route {...rest}>{user ? children : <Redirect to={`/login`} />}</Route>\n  );\n}\n","/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/components/Header.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/components/Spinner.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/utils/validationFuncs.js",["104","105","106","107","108"],"const Joi = require(\"joi\");\n\nconst cpfRegex = /([0-9]{2}[\\.]?[0-9]{3}[\\.]?[0-9]{3}[\\/]?[0-9]{4}[-]?[0-9]{2})|([0-9]{3}[\\.]?[0-9]{3}[\\.]?[0-9]{3}[-]?[0-9]{2})/;\n\nconst userSignUpSchema = Joi.object({\n  email: Joi.string()\n    .email({ tlds: { allow: false } })\n    .lowercase()\n    .required(),\n  cpf: Joi.string().pattern(cpfRegex).required().messages({\n    \"string.pattern.base\": \"formato de CPF invalido\",\n  }),\n  password: Joi.string().min(2).max(30).required(),\n  confirmPassword: Joi.any().valid(Joi.ref(\"password\")).required().messages({\n    \"any.only\": \"Confirmacao de senha diferente\",\n  }),\n});\n\nconst userSignInSchema = Joi.object({\n  email: Joi.string()\n    .email({ tlds: { allow: false } })\n    .lowercase()\n    .required(),\n  password: Joi.string().min(2).max(30).required().messages({\n    \"string.base\": `Tem que ser texto`,\n    \"string.min\": \"Senha tem que ser maior que 2 carc\",\n  }),\n});\n\nconst validateSignup = (userForm) => {\n  if (userSignUpSchema.validate(userForm, { abortEarly: false }).error) {\n    throw new Error(\n      userSignUpSchema.validate(userForm, { abortEarly: false }).error.message\n    );\n  }\n};\n\nconst validateSignin = (email, password) => {\n  if (\n    userSignInSchema.validate({ email, password }, { abortEarly: false }).error\n  ) {\n    throw new Error(\n      userSignInSchema.validate(\n        { email, password },\n        { abortEarly: false }\n      ).error.message\n    );\n  }\n};\n\nmodule.exports = { validateSignup, validateSignin };\n","/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/utils/Button.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/utils/Form.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/hooks/useLocalStorage.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/Registration.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/PreRegistration/PreRegistration.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/RegistrationPage/Registration.js",["109"],"import React, { useContext, useEffect, useState } from \"react\";\nimport styled from \"styled-components\";\nimport axios from 'axios';\n\nimport Header from '../../components/Header';\nimport PersonalData from \"./PersonalData\";\nimport TicketData from \"./TicketData\";\nimport HotelData from './HotelData'\nimport { userContext } from \"../../contexts/UserContext\";\n\nexport default function Registration() {\n  const [personalData, serPersonalData] = useState(null);\n  const [isHotel, setIsHotel] = useState(false);\n  const [page, setPage] = useState(1);\n  const { user } = useContext(userContext);\n  console.log(personalData);\n\n  useEffect(() => {\n    const request = axios.get('http://localhost:3000/api/registration/ticket', {headers: {\"x-access-token\": user.token}});\n    request.then(response =>{\n      if(response.data === 'hotel') setIsHotel(true);\n      else setIsHotel(false);\n    }).catch(err => {\n      console.log(err.response);\n    })\n  })\n\n  \n  return(\n    <>\n      <Header />\n      <RegContainer>\n          {\n            page === 1 ?\n            <PersonalData personalData={personalData} setPersonalData = {serPersonalData} setPage={setPage} setIsHotel={setIsHotel} isHotel = {isHotel} /> :\n            null\n          }\n          {\n            isHotel && page === 2 ?\n            <HotelData personalData={personalData} setPersonalData = {serPersonalData} setPage={setPage} setIsHotel={setIsHotel} isHotel = {isHotel} />:\n            null\n          }\n      </RegContainer>\n    </>\n  )\n}\n\n\nconst RegContainer = styled.div`\n  width: 100vw;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n\n  h2{\n    font-size: 40px;\n    margin: 30px 0;\n  }\n`;\n\n","/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/PreRegistration/TicketSelection.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/RegistrationPage/PersonalData.js",["110"],"import React from 'react';\nimport Form from '../../utils/Form';\nimport Button from '../../utils/Button';\nimport {useForm, Controller} from 'react-hook-form';\nimport { useHistory } from 'react-router-dom';\n\nexport default function PersonalData({personalData, setPersonalData, setPage, setIsHotel, isHotel}){\n  const { register, handleSubmit, control, errors } = useForm();\n  const history = useHistory();\n\n  const onSubmit = data => {\n    if(personalData && personalData.hasOwnProperty('hotel')){\n      const hotel = personalData.hotel;\n      setPersonalData({...data, hotel});\n    }else setPersonalData(data);\n    setPage(2);\n  }\n\n  return(\n      <>\n      <h2>Confirmar Inscrição</h2>\n      <Form value='flex-end' onSubmit = {handleSubmit(onSubmit)}>\n         <h3>Dados pessoais</h3>\n         <Controller\n            as={<input />}\n            control={control}\n            name='name' \n            placeholder=\"Nome Completo\" \n            defaultValue={personalData ? personalData.name : \"\"} \n            rules = {{\n              required: true,\n                minLength: 3,\n              }}\n           />\n            {errors.name && errors.name.type === 'required' && <p>Preencha este campo!</p>}\n            {errors.name && errors.name.type === 'minLength' && <p>Entrada inválida!</p>}\n            <Controller \n              as={<input />}\n              control={control}\n              name='address' \n              placeholder=\"Endereço\"\n              defaultValue={personalData ? personalData.address : \"\"} \n              rules = {{\n                required: true,\n                minLength: 3,\n              }}\n            />\n            {errors.address && errors.address.type === 'required' && <p>Preencha este campo!</p>}\n            {errors.address && errors.address.type === 'minLength' && <p>Entrada inválida!</p>}\n            <Controller \n              as={<input />}\n              control={control}\n              name='phone' \n              placeholder=\"Fone\"\n              defaultValue={personalData ? personalData.phone : \"\"} \n              rules = {{\n                required: true,\n                minLength: 8,\n              }}\n            />\n            {errors.phone && errors.phone.type === 'required' && <p>Preencha este campo!</p>}\n            {errors.phone && errors.phone.type === 'minLength' && <p>Entrada inválida!</p>}\n           <select name='gender' ref={register({ required: true })}>\n              <option value=\"\">Gênero</option>\n              <option value=\"female\">Feminino</option>\n              <option value=\"male\">Masculino</option>\n              <option value=\"others\">Outros</option>\n              <option value=\"ratherNotSay\">Prefiro não informar</option>\n           </select>\n           {errors.gender && errors.gender.type === 'required' && <p>Preencha este campo!</p>}\n           <div className= 'btn-container'>\n              <Button type='submit'>{isHotel ? 'Próximo' : 'Finalizar'}</Button>\n           </div>\n        </Form>  \n        </>\n    )\n}","/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/RegistrationPage/TicketData.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/pages/RegistrationPage/HotelData.js",[],"/Users/paolahoffmann/Desktop/camps-party-front/camps-party-front/src/utils/Label.js",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":26,"column":6,"nodeType":"117","endLine":26,"endColumn":16,"suggestions":"118"},{"ruleId":"115","severity":1,"message":"119","line":12,"column":6,"nodeType":"117","endLine":12,"endColumn":8,"suggestions":"120"},{"ruleId":"121","severity":1,"message":"122","line":3,"column":29,"nodeType":"123","messageId":"124","endLine":3,"endColumn":30,"suggestions":"125"},{"ruleId":"121","severity":1,"message":"122","line":3,"column":42,"nodeType":"123","messageId":"124","endLine":3,"endColumn":43,"suggestions":"126"},{"ruleId":"121","severity":1,"message":"127","line":3,"column":55,"nodeType":"123","messageId":"124","endLine":3,"endColumn":56,"suggestions":"128"},{"ruleId":"121","severity":1,"message":"122","line":3,"column":91,"nodeType":"123","messageId":"124","endLine":3,"endColumn":92,"suggestions":"129"},{"ruleId":"121","severity":1,"message":"122","line":3,"column":104,"nodeType":"123","messageId":"124","endLine":3,"endColumn":105,"suggestions":"130"},{"ruleId":"131","severity":1,"message":"132","line":7,"column":8,"nodeType":"133","messageId":"134","endLine":7,"endColumn":18},{"ruleId":"131","severity":1,"message":"135","line":9,"column":9,"nodeType":"133","messageId":"134","endLine":9,"endColumn":16},